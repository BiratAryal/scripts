            },
            "keys": "ctrl+alt+down"
        },
        {
            "command": "paste",
            "keys": "ctrl+v"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "auto",
                "splitMode": "duplicate"
            },
            "keys": "alt+shift+d"
        },
        {
            "command": "closePane",
            "keys": "ctrl+w"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 3
            },
            "keys": "ctrl+s"
        },
        {
            "command": 
            {
                "action": "splitPane",
                "split": "vertical"
            },
            "keys": "ctrl+alt+up"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 4
            },
            "keys": "ctrl+i"
        },
        {
            "command": 
            {
                "action": "newTab",
                "index": 2
            },
            "keys": "ctrl+m"
        }
